{
  "name": "@pwndao/sdk-v1-react",
  "version": "0.0.1-alpha",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "tags": [
      "v1 pwn-sdk"
    ],
    "targets": {
      "biome-lint": {
        "executor": "@gitopslovers/nx-biome:biome-lint",
        "outputs": [
          "{options.outputFile}"
        ],
        "options": {
          "lintFilePatterns": [
            "packages/v1-sdk-react/**/*.ts"
          ],
          "write": false,
          "unsafe": false
        }
      }
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@pwndao/sdk-core": "workspace:*",
    "@pwndao/v1-core": "workspace:*",
    "vitest": "^1.3.1",
    "wagmi": "^2.14.13"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.67.3",
    "react": "^19"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^14.2.1",
    "jsdom": "^24.0.0",
    "prettier": "^2.6.2",
    "react": "19.0.0",
    "vitest": "^1.3.1"
  }
}
